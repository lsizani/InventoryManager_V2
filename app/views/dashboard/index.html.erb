<h1>Perform Admin Task</h1>

<h2>All Studies in Logs</h2>

<table align="center">
  <thead>
       <td>Study ID</td>
       <td>Study Name</td>
       <td>Study Number</td>
       <td>Study Start Date</td>
  </thead>
  <tbody>
    <% @studies.each do |study| %>
       <tr>
         <td><%= study.id %></td>
         <td><%= study.study_name %></td>
         <td><%= study.study_number %></td>
         <td><%= study.study_start_date %></td>
       </tr>
    <% end %>
  </tbody>
</table>

<div class="navBox">
  <%= link_to 'Add Study', :controller => 'studies', :action => 'new'  %>  |
  <%= link_to 'Home', :controller => 'welcome', :action => 'index'  %>
</div>